INPUT score
IF score >= 70 AND score <= 100 THEN
    grade = A
ELSE IF score >= 60 AND score < 70 THEN
    grade = B
ELSE IF score >= 50 AND score < 60 THEN
    grade = C
ELSE IF score >= 45 AND score < 50 THEN
    grade = D
ELSE IF score >= 40 AND score < 45 THEN
    grade = E
ELSE
    grade = F
END IF
OUTPUT grade
